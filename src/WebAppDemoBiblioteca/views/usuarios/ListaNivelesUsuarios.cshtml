@model ListaNivelesUsuariosViewModel

@foreach (var NivelesUsuario in Model.NivUsuarios)
{
    <div class="col-lg-3 col-md-6 mb-4">
        <div class="card">
            <img class="card-img-top" src="" alt="">
            <div class="card-body">
                <h4 class="card-title">@NivelesUsuario.NombreNivel</h4>
                
            </div>
            <div class="card-footer">
                <a href="#" class="btn btn-primary">Detalles</a>
            </div>
        </div>
    </div>
}